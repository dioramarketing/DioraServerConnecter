{
  "name": "dioraserverconnecter",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "shared",
    "server",
    "dashboard"
  ],
  "scripts": {
    "dev:server": "npm run dev -w server",
    "build:server": "npm run build -w server",
    "dev:dashboard": "npm run dev -w dashboard",
    "build:dashboard": "npm run build -w dashboard",
    "build:all": "npm run build -w shared && npm run build -w dashboard && npm run build -w server",
    "db:migrate": "npm run db:migrate -w server",
    "db:generate": "npm run db:generate -w server"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
